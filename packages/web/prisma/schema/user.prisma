model User {
  createdAt DateTime @default(now())
  expiredAt DateTime
  id        Int      @id @default(autoincrement())
  name      String

  conversations        ConversationXParticipant[]
  initiatorDialogues   Dialogue[]                 @relation("initiator")
  participantDialogues Dialogue[]                 @relation("participant")

  @@map("users")
}
