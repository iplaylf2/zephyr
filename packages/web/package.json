    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.13",
    "@zephyr/config": "workspace:*",
    "eslint": "^9.18.0",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
