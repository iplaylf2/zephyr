    "@nestjs/platform-express": "^11.0.3",
    "@nestjs/swagger": "^11.0.2",
    "@prisma/client": "6.2.1",
    "@redis/client": "^1.6.0",
    "@zephyr/kit": "workspace:*",
    "defaults": "^3.0.0",
    "dotenv": "^16.4.7",
    "effection": "^3.2.0",
