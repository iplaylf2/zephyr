{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "init": {
            "dependsOn": [
                "^init",
                "^build"
            ],
            "cache": false
        },
        "lint": {}
    }
}